@section PageStyles{
    <link href="~/assets/static/plugins/select2/dist/css/select2.min.css" rel="stylesheet" />
    <link href="~/assets/static/plugins/jstree/dist/themes/default/style.min.css" rel="stylesheet" />
    <link href="~/assets/static/plugins/bootstrap-sweetalert2/sweetalert2.min.css" rel="stylesheet" />
    <link href="~/assets/static/plugins/jstree/v321/dist/style.min.css" rel="stylesheet" />
}
<!-- begin page-header -->
<h1 class="page-header"><i class="fa fa-eye"></i> Access Control <small> Role Menu Access</small></h1>
<!-- end page-header -->
<div class="panel panel-inverse">
    <div class="panel-heading">
        <div class="panel-heading-btn">
            <a href="@Url.Action("MenuAccess", "AccessControl")" class="btn btn-xs btn-icon btn-circle btn-success" data-click="panel-reload"><i class="fa fa-repeat"></i></a>
            <a href="@Url.Action("Index", "Dashboard")" class="btn btn-xs btn-icon btn-circle btn-danger" data-click="panel-remove"><i class="fa fa-times"></i></a>
        </div>
        <h4 class="panel-title">Role Menu Access</h4>
    </div>
    <div class="panel-body">
        <p></p>
        <div class="form-horizontal">
            <div class="form-group">
                <label class="col-md-3 control-label">Profile</label>
                <div class="col-md-6">
                    <select id="profile" name="profile" class="form-control selectpicker" style="width: 100%"></select>
                </div>
            </div>
            <div class="form-group">
                <label class="col-md-3 control-label">Menu</label>
                <div class="col-md-6">
                    <div class="jumbotron">
                        <div id="tree-menu"></div>
                    </div>
                </div>
            </div>
            <div class="form-group">
                <br />
                <br />
                <div class="text-center">
                    <button type="submit" class="btn btn-success btn-sm" value="submit" name="save" id="save">
                        <i class="fa fa-save"></i>&nbsp;Save
                    </button>
                </div>
            </div>
        </div>
        <hr>
    </div>
    <!-- end panel-body -->
</div>
<!-- end panel -->
@section PageScripts{
    <script src="~/assets/static/plugins/select2/dist/js/select2.min.js"></script>
    <script src="~/assets/static/js/bootstrap-treeview.js"></script>
    <script src="~/assets/static/plugins/jstree/v321/dist/jstree.min.js"></script>
    <script src="~/assets/static/plugins/bootstrap-sweetalert2/sweetalert2.all.min.js"></script>
    <script src="~/assets/static/js/apps.min.js"></script>
    <script src="~/assets/static/js/jquery.blockUI.js"></script>
    <script src="~/assets/static/js/access-control/menu-access.js"></script>


   


}